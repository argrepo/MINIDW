.content-right form .btn.btn-dark {
	border-radius: 3px;
	background: #464646;
	padding: 9px 15px;
	color: #fff;
	font-weight: 700;
	font-size: 11px;
	position: absolute;
	bottom: 0;
	right: 15px;
}

.content-right form .btn.btn-dark.rel {
	right: inherit;
}

.content-right form .btn.btn-dark:hover {
	background: #3a3a3a;
}

