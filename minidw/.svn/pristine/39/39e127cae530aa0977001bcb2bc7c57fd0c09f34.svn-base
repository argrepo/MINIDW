footer {
	background: #252525;
	height: 100px;
	color: #999999;
	font-weight: 700;
	font-size: 16px;
	padding-top: 40px;
	position: fixed;
	bottom: 0;
	width: 100%;
}

footer .wizard {
	padding-left: 47px;
}

footer .wizard span {
	display: inline-block;
	width: 188px;
}

footer .wizard li {
	float: left;
	list-style-type: none;
	height: 15px;
	width: 15px;
	background: #a1a1a1;
	border: 1px solid #252525;
	border-radius: 7.5px;
	z-index: 2;
	position: relative;
	margin-right: 108px;
}

footer .wizard li.active {
	background: #0099ff;
}

footer .wizard li:last-child {
	margin-right: -4px;
}

footer .wizard ul {
	position: relative;
	margin: 0;
	padding: 0;
}

footer .wizard ul:before {
	content: '';
	position: absolute;
	top: 40%;
	bottom: 0;
	background: #636363;
	left: 20%;
	margin-left: -74px;
	height: 3px;
	width: 100%;
	z-index: 1;
}