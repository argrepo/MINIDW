package com.datamodel.anvizent.service.model;

public class QueryBuilderForm {
	
	private String packageId, industryId;
	private boolean isstaging;
	private boolean advanced;
	 
	
	public String getPackageId() {
		return packageId;
	}

	public void setPackageId(String packageId) {
		this.packageId = packageId;
	}

	public String getIndustryId() {
		return industryId;
	}

	public void setIndustryId(String industryId) {
		this.industryId = industryId;
	}

	public boolean isIsstaging() {
		return isstaging;
	}

	public void setIsstaging(boolean isstaging) {
		this.isstaging = isstaging;
	}

	public boolean isAdvanced() {
		return advanced;
	}

	public void setAdvanced(boolean advanced) {
		this.advanced = advanced;
	}

	@Override
	public String toString() {
		return "QueryBuilderForm [packageId=" + packageId + ", industryId=" + industryId + ", isstaging=" + isstaging
				+ ", advanced=" + advanced + "]";
	}
	
}
