package com.datamodel.anvizent.service.model;

public class TableScripts {

	private int id;
	private int priority;
	private String scriptName;
	private String sqlScript;
	private Modification modification;
	private boolean is_Update;
	private boolean is_Active;
	private String scriptType;
	private boolean checkedScript;
	private boolean isExecuted;
	private String targetSchema;
	private String created_Date;
	private String modified_Date;
	private boolean isError;
	private Boolean isDefault = false;
	private String version;
	public TableScripts() {
	}

	public TableScripts(int id, int priority, String scriptName,String sqlScript,Modification modification,boolean is_Update,boolean is_Active,String scriptType,boolean checkedScript,boolean isExecuted,String targetSchema,String created_Date,String modified_Date,boolean isError) {
		this.id=id ;
		this.priority=priority;
		this.scriptName=scriptName; 
		this.sqlScript=sqlScript; 
		this.modification=modification;
		this.is_Update=is_Update;
		this.is_Active=is_Active;
		this.scriptType=scriptType;
		this.checkedScript=checkedScript;
		this.isExecuted=isExecuted;
		this.targetSchema = targetSchema;
		this.created_Date = created_Date;
		this.modified_Date=modified_Date;
		this.isError=isError;
	}
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
	public int getPriority() {
		return priority;
	}
	public void setPriority(int priority) {
		this.priority = priority;
	}
	public String getScriptName() {
		return scriptName;
	}
	public void setScriptName(String scriptName) {
		this.scriptName = scriptName;
	}
	public String getSqlScript() {
		return sqlScript;
	}
	public void setSqlScript(String sqlScript) {
		this.sqlScript = sqlScript;
	}
	
	public Modification getModification() {
		return modification;
	}
	public void setModification(Modification modification) {
		this.modification = modification;
	}
 
	public boolean isIs_Update() {
		return is_Update;
	}
	public void setIs_Update(boolean is_Update) {
		this.is_Update = is_Update;
	}
	
	public boolean isIs_Active() {
		return is_Active;
	}
	public void setIs_Active(boolean is_Active) {
		this.is_Active = is_Active;
	}
	public String getScriptType() {
		return scriptType;
	}
	public void setScriptType(String scriptType) {
		this.scriptType = scriptType;
	}

	public boolean isCheckedScript() {
		return checkedScript;
	}

	public void setCheckedScript(boolean checkedScript) {
		this.checkedScript = checkedScript;
	}

	public boolean isExecuted() {
		return isExecuted;
	}

	public void setExecuted(boolean isExecuted) {
		this.isExecuted = isExecuted;
	}

	public String getTargetSchema() {
		return targetSchema;
	}

	public void setTargetSchema(String targetSchema) {
		this.targetSchema = targetSchema;
	}

	public String getCreated_Date() {
		return created_Date;
	}

	public void setCreated_Date(String created_Date) {
		this.created_Date = created_Date;
	}

	public String getModified_Date() {
		return modified_Date;
	}

	public void setModified_Date(String modified_Date) {
		this.modified_Date = modified_Date;
	}

	public boolean isError() {
		return isError;
	}

	public void setError(boolean isError) {
		this.isError = isError;
	}

	public Boolean getIsDefault() {
		return isDefault;
	}

	public void setIsDefault(Boolean isDefault) {
		this.isDefault = isDefault;
	}

	public String getVersion() {
		return version;
	}

	public void setVersion(String version) {
		this.version = version;
	}

	@Override
	public String toString() {
		return "TableScripts [id=" + id + ", priority=" + priority + ", scriptName=" + scriptName + ", sqlScript="
				+ sqlScript + ", modification=" + modification + ", is_Update=" + is_Update + ", is_Active=" + is_Active
				+ ", scriptType=" + scriptType + ", checkedScript=" + checkedScript + ", isExecuted=" + isExecuted
				+ ", targetSchema=" + targetSchema + ", created_Date=" + created_Date + ", modified_Date="
				+ modified_Date + ", isError=" + isError + ", isDefault=" + isDefault + ", version=" + version + "]";
	}
 
	 
}
