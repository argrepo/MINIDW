package com.datamodel.anvizent.service.model;

public class ClientDataSources {

	private long id;
	private long clientId;
	private String dataSourceName;
	private String dataSourceFrom;
	private Modification modification;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public long getClientId() {
		return clientId;
	}

	public void setClientId(long clientId) {
		this.clientId = clientId;
	}

	public String getDataSourceName() {
		return dataSourceName;
	}

	public void setDataSourceName(String dataSourceName) {
		this.dataSourceName = dataSourceName;
	}
	public ClientDataSources(long clientId, String dataSourceName) {
		this.clientId = clientId;
		this.dataSourceName = dataSourceName;
	}
	public ClientDataSources() {
	}
	
	public String getDataSourceFrom() {
		return dataSourceFrom;
	}

	public void setDataSourceFrom(String dataSourceFrom) {
		this.dataSourceFrom = dataSourceFrom;
	}

	public Modification getModification() {
		return modification;
	}

	public void setModification(Modification modification) {
		this.modification = modification;
	}

	@Override
	public String toString() {
		return "ClientDataSources [id=" + id + ", clientId=" + clientId + ", dataSourceName=" + dataSourceName
				+ ", dataSourceFrom=" + dataSourceFrom + ", modification=" + modification + "]";
	}

}
